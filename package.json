{
  "name": "ground",
  "productName": "Ground Station",
  "main": "electron/main.js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "circular-buffer": "~1.0.3",
    "electron-is-dev": "~1.2.0",
    "influx": "~5.9.2",
    "jsonc": "^2.0.0",
    "lodash.throttle": "~4.1.1",
    "moment": "~2.29.1"
  },
  "scripts": {
    "start": "cross-env electron .",
    "start-main": "cross-env VARIANT=main electron .",
    "start-aux": "cross-env VARIANT=aux electron .",
    "build-electron-main": "cp -r telemetry/build ./build && cp -r electron/. build/electron",
    "package": "electron-builder build --mac -c.extraMetadata.main=build/electron/main.js --publish never",
    "test": "mocha"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "ground",
      "owner": "Space-Enterprise-at-Berkeley"
    }
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-rebuild": "^2.2.0",
    "mocha": "^8.3.2"
  }
}
